    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Generate folds with pattern",
            "type": "python",
            "request": "launch",
            "program": "generateImageFoldsWithPattern.py",
            "console": "integratedTerminal",
            "args":[
                "-p","micc_newdepth_vgg",
                "--folder","folds_micc_vgg",
                "--filePattern","folds_file_vgg.txt",
                "--extension","png"
            ]
        },
        {
            "name": "Python: Recognition With DB",
            "type": "python",
            "request": "launch",
            "program": "Recognition_withdb.py",
            "console": "integratedTerminal",
            "args":[
                "--path","features/giogio_bosphorus_frgc_depth_di_nofus_miccdataset.txt",
                "--folds","folds_file_micc.txt",
                "--saveScores","scores/giogioinputkernelattdi_sigmoid_1fc"
            ]
        },
        {
            "name": "Python: Extract Deep Features",
            "type": "python",
            "request": "launch",
            "program": "extractDeepFeaturesPytorch.py",
            "console": "integratedTerminal",
            "args": [
                "--loadFromFolder","generated_images_eurecom_sigmoid",
                "--fineTuneWeights","bosphorus_frgc_attentionINCN_sigmoid/attentionINCN_best_loss.pth.tar",
                "--output","features/attention_cross_bosphorus_frgc_paper_ab.txt",
                "--network","attentionINCN",
                "--modeLoadFile","RGBA",
                "--meanImage",
                "0.7480380159512608",
                "0.4296281369158474",
                "0.47537796754456024",
                "0.3815522899811688",
                "--stdImage",
                "0.3389922561039528",
                "0.43293648195263507",
                "0.37572992845338415",
                "0.33357417170101733",
                "--depthFolder",
                "eurecom_newdepth_dataset_renamed"
            ]
        },
        {
            "name": "Python: MICC Preprocess dataset",
            "type": "python",
            "request": "launch",
            "program": "mainProgramMICC.py",
            "console": "integratedTerminal",
            "args": [
                "-p=D:\\MICC Dataset",
                "-t=ply",
                "-op=pp",
                "--faceVariation=N_LFAU_UFAU_E_CAU",
                "--quantityProcesses=4",
                "--pathImages=generated_images_bosphorus",
                "-s=InitialFilter_TranslateFix:nosetip=0_GenerateNewDepthMapsMICC"
            ]
        },
        {
            "name": "Python: Train network with full data",
            "type": "python",
            "request": "launch",
            "program": "trainWithFullDataPytorch.py",
            "console": "integratedTerminal",
            "args": [
                "-c=563",
                "-p=training_bosphorus_frgc_sigmoid",
                "-e=200",
                "-b=25",
                "--output=bosphorus_frgc_attentionINCN_sigmoid",
                "--extension=png",
                "--network=attentionINCN",
                "--tensorboardname=frgc_giogio_sigmoid_kernel_input",
                "--optimizer=adam",
                "--learningRate=0.00005",
                "--meanImage",
                "0.7480380159512608",
                "0.4296281369158474",
                "0.47537796754456024",
                "0.3815522899811688",
                "--stdImage",
                "0.3389922561039528",
                "0.43293648195263507",
                "0.37572992845338415",
                "0.33357417170101733",
                "--depthFolder=bosphorus_frgc_joineddepth"
            ]
        },
    ]
}